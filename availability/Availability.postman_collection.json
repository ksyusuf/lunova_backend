			"item": [
				{
					"name": "Uzman Takvimini Görüntüle",
					"request": {
						"method": "GET",
						"header": [
							{ "key": "Cookie", "value": "access_token={{access_token}}" }
						],
						"url": {
							"raw": "{{base_url}}/api/v1/availability/expert/{{expert_id}}/calendar/?start_date={{start_date}}&end_date={{end_date}}",
							"host": [ "{{base_url}}" ],
							"path": [ "api", "v1", "availability", "expert", "{{expert_id}}", "calendar", "" ],
